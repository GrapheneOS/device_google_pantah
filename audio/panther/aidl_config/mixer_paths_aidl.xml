  <ctl name="HAPTIC_RX SlotFmt" value="S32_LE" />
  <ctl name="HAPTIC_RX Chan" value="Four" />
  <ctl name="HAPTIC_RX nSlot" value="Four" />
  <ctl name="RX Slots" id="0" value="2" />
  <ctl name="RX Slots" id="1" value="3" />

  <!-- Cirrus default 96K -->
  <ctl name="Default 96K" value="0" />
