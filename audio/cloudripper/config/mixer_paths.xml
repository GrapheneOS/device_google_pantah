    <ctl name="Sidetone Biquad2" id="4" value="0"/>
    <ctl name="Sidetone Biquad2" id="5" value="0"/>
    <ctl name="Sidetone Biquad3" id="0" value="0"/>
    <ctl name="Sidetone Biquad3" id="1" value="0"/>
    <ctl name="Sidetone Biquad3" id="2" value="0"/>
